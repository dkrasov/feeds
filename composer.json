{
    "name": "dkrasov/feeds",
    "description": "Laravel 5 Service Provider for the SimplePie library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Krasov",
            "email": "dskrasov@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "6.0.*",
        "simplepie/simplepie": "1.5.*",
        "ext-curl": "*"
    },
    "autoload": {
